structure (LocalTeamDetails) {
  description (Score and other details of the local team)
  property (Name) {
    type (LocalTeamName)
    min (Required) max (One)
  }
  property(ImageUrl){
    type (ImageUrl)
    min(Required) max(One)
  }
  property(Runs){
    type (Runs)
    min(Optional) max(One)
  }
  property(Wickets){
    type (Wickets)
    min(Optional) max(One)
  }
  property(Overs){
    type (Overs)
    min(Optional) max(One)
  }
}
